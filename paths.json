{
  "build":"./build/",
  "src":"./src/",
  "clean":[
    "build/**/*",
    "!build/vendor",
    "!build/vendor/*",  
    "!build/vendor/**/*"
  ],
  "styles":{
    "compile":["./src/**/*.scss"],
    "build":"./build/",
    "cacheName":"styles"
  },
  "templates": {
    "compile": ["!./src/app/views/**/*.html","!./src/app/views/*.html","./src/app/**/*.html"],
    "build":"./build/templates/"
  },
  "views":{
    "compile": ["./src/app/views/**/*.html","./src/app/views/*.html"],
    "build":"./build/"
  },
  "scripts": {
    "cacheName": "scripts",
    "compile": [
      "./src/app/**/*.module.js",
      "./src/app/**/*.constants.js",
      "./src/app/**/*.config.js",
      "./src/app/**/*.run.js",
      "./src/app/**/*.controller.js",
      "./src/app/**/*.component.js",
      "./src/app/**/*.service.js",
      "./src/app/**/*.filter.js",
      "./src/app/**/*.js"
    ],
    "build": "./build/app/"
  }
}